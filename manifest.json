{
	"manifest_version": 2,
	"name": "MIPT-Telecom",
	"version": "1.2",
	"description": "Баланс на счету МФТИ-Телеком",
	"background":
	{
		"scripts":
		[
			"background.js"
		],
		"persistent": false
	},
	"icons":
	{
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action":
	{
		"default_icon": "icon.png",
		"default_popup": "options.html"
	},
	"permissions":
	[
		"http://cabinet.telecom.mipt.ru/",
		"alarms"
	]
}
